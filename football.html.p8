function new_game()
	local g = {
		turn = 1,
	}
	
	g.score = {}
	g.score[1] = 0
	g.score[2] = 0

	g.width=128
	g.height=128

	g.pitch={}
	g.pitch.width=14*8
	g.pitch.height=9*8
	g.pitch.top_border=2*8
	g.pitch.side_border=1*8

	g.pitch.xstart=(g.pitch.side_border)
	g.pitch.xend=(g.pitch.side_border+g.pitch.width)
	g.pitch.ystart=(g.pitch.top_border)
	g.pitch.yend=(g.pitch.top_border+g.pitch.height)

	g.pitch.xmid=(g.pitch.width/2) + g.pitch.side_border
	g.pitch.ymid=(g.pitch.height/2) + g.pitch.top_border

	g.goalpost_left = {line=g.pitch.xstart, top_post=g.pitch.ystart+(3*8), bottom_post=g.pitch.yend-(3*8)}
	g.goalpost_right = {line=g.pitch.xend, top_post=g.pitch.ystart+(3*8), bottom_post=g.pitch.yend-(3*8)}

	g.add_goal = function(team)
		g.score[team] += 1
	end
	
	g.get_score = function()
		return g.score
	end
	
	g.end_turn = function()
		if g.turn == 1 then g.turn = 2
		else g.turn = 1
		end
	end
	
	g.get_turn = function()
		return g.turn
	end
		
	g.end_game = function(winner) 
		print("game over")
	end		
	
	sfx(0)
	return g
end
