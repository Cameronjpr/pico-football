function new_player(t,n)
	local p = {}
	
 p.spr = t-1
 p.t = t
 p.n = n
 p.move_radius=4*8
 
	if n == 5 then
		p.spr=2
		p.x=teams[t]["gk_pos"]
		p.y=(g.pitch.height/2)+8+4
 else 
		p.x=teams[t]["pos"]
		p.y=((g.pitch.ystart - g.pitch.top_border) + (n*16) + 8)
	end

 p.draw = function(this)
	if (this == c.slct) then
		this.draw_move_radius(this)
	end
	flipped=b.x>this.x
  spr(this.spr, this.x, this.y, 1, 1, flipped)
 end

 p.draw_move_radius = function(this)
	circ(this.x+4, this.y+4, this.move_radius, 5)
 end
 
 p.update = function(this,x,y)
	this.x=x
	this.y=y
 end

 
 p.check_if_hovered = function(this)
 	x_hov=false
 	y_hov=false
 	
 	if (c.x+4 >= this.x and c.x+4 < this.x + 8) then
 		x_hov=true
 	end

 	if (c.y+4 >= this.y and c.y+4 < this.y + 8)	then
 		y_hov=true
 	end
 		
 	return x_hov and y_hov
 end			

 p.make_pass = function(this)
	in_poss={}
 end

 p.receive_pass = function(this)
	in_poss=this
 end
 	
 return p
end
