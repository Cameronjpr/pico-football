function new_infobar()
	local ib={}

	ib.score={
		xstart=g.width/2-6,
		xend=(g.width/2)+5,
		ystart=(g.pitch.top_border/2)+1,
		color=7
	}

	ib.teams={}
	ib.teams[1]={
		xstart=ib.score.xstart-8-1,
		ystart=ib.score.ystart-1,
		spr=48
	}

	ib.teams[2]={
		xstart=ib.score.xend+1,
		ystart=ib.score.ystart-1,
		spr=49
	}

	ib.log_config={
		xstart=g.pitch.width/2,
		ystart=g.pitch.height+g.pitch.top_border
	}

	ib.format_score=function()
		return tostr(g.score[1]) .. '-' .. tostr(g.score[2])
	end

	ib.draw_score=function()
		print(ib.format_score(),ib.score.xstart,ib.score.ystart,ib.score.color)
	end

	ib.draw_teams=function()
		if (g.turn == 1) then
			spr(ib.teams[1].spr,ib.teams[1].xstart,ib.teams[1].ystart)
			spr(50,ib.teams[2].xstart,ib.teams[2].ystart)
		elseif (g.turn == 2) then
			spr(50,ib.teams[1].xstart,ib.teams[1].ystart)
			spr(ib.teams[2].spr,ib.teams[2].xstart,ib.teams[2].ystart)
		end
	end

	ib.draw_info_board=function()
		ib.draw_score()
		ib.draw_teams()
	end

	ib.log=function(msg)
		print(tostr(msg), ib.log_config.xstart, ib.log_config.ystart)
	end

	return ib
end
