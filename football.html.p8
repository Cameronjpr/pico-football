function new_cursor()
	local c={
		x=g.pitch.xmid-8,
		y=g.pitch.ymid-8,
		spd=2
	}
	
	// hovered
	c.hvrd = {}
	
	// selected
	c.slct = nil
	
	// destination
	c.dest= nil

	c.clear_all = function()
		c.hvrd=nil
		c.slct=nil
		c.dest=nil
	end

	c.push_cursor_back_inside_radius=function(circx,circy)
		if(c.x > circx) then
			c.x-=1
		else
			c.x+=1
		end

		if(c.y > circy) then
			c.y-=1
		else
