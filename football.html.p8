function new_utils()
	local u={}

	u.is_point_inside_circle=function(px,py,cx,cy,rad)
		dist = util.get_hypotenuse(abs(px-cx),abs(py-cy))
		return dist < rad
	end

	--maffs is cool
	u.get_hypotenuse=function(a,b)
		return sqrt((a*a) + (b*b))
	end

	return u
end

