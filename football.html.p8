function new_ball()
	local b={}
	
	b.diam=8
	b.x=64
	b.y=32
	b.hov=false

	b.update = function(x,y)
		b.x=x
		b.y=y
	end
	
	b.draw=function()
		if (bhov) then
			spr(19,b.x,b.y)
		else
			spr(16,b.x,b.y)
		end
	end
	
	b.is_goal=function(this)
		goal=nil
		if (b.x <= g.goalpost_left.line) then
			if (b.y < g.goalpost_left.bottom_post and b.y > g.goalpost_left.top_post) then
				goal=2 --left, team1
			end
		elseif (b.x+8 >= g.goalpost_right.line) then
			if (b.y < g.goalpost_right.bottom_post and b.y > g.goalpost_right.top_post) then
				goal=1 --right, team2
			end
		end
		return goal
	end

	b.check_if_line_of_sight_clear = function()

	end

	b.check_if_hovered = function(this)
		x_hov=false
 		y_hov=false
 	
		if (c.x+4 >= this.x and c.x+4 < this.x + 8) then
			x_hov=true
		end

		if (c.y+4 >= this.y and c.y+4 < this.y + 8)	then
			y_hov=true
		end
			
		b.hov=(x_hov and y_hov)	
		return x_hov and y_hov	
	end
	
	return b
end
