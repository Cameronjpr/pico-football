function _init()
	g = new_game()
	c = new_cursor()
	util = new_utils()
	info = new_infobar()
	action_type=''

	in_poss={}
	
	teams = {}
	teams[1] = {}
	teams[2] = {}

	teams[1]["pos"] = g.pitch.xstart + g.pitch.side_border*2
	teams[2]["pos"] = g.pitch.xend - g.pitch.side_border*2-8
	teams[1]["gk_pos"] = g.pitch.side_border+4
	teams[2]["gk_pos"] = (g.pitch.width -  g.pitch.side_border)+4

	for x=1,5 do
		teams[1][x] = new_player(1,x);
		teams[2][x] = new_player(2,x);
	end
	
	b = new_ball()
	b.x = teams[1][2].x + 8
	b.y = teams[1][2].y
	in_poss = teams[1][2]
end

function draw_all_players()
	for t in all(teams) do
  	for p in all(t) do
			flipped=false
			if (b.x>p.x) then flipped=true end

			// hovered player
			if (p == c.hvrd) then
				spr(3,p.x,p.y,1,1,flipped)
			else
  			p.draw(p)
			end
			
			// selected player
			if (p == c.slct) then
				spr(4,p.x,p.y)
			// destination player 
		 	elseif (p == c.dest) then
				spr(5,p.x,p.y)
			end

			--possesion indicator
			if (p == in_poss) then
				spr(6,p.x,p.y-8)
			end
  	end
	end
end
	
function _update()
	c.hvrd=nil

	if (btn(ðŸ…¾ï¸)) then
		c.clear_all()
	end

	--restrict cursor movement if selection has been made
	if(c.slct and c.slct.move_radius != nil) then
		if (util.is_point_inside_circle(c.x,c.y,c.slct.x,c.slct.y,c.slct.move_radius)) then
			if (btn(â¬…ï¸)) c.x-=c.spd
			if (btn(âž¡ï¸)) c.x+=c.spd
			if (btn(â¬†ï¸)) c.y-=c.spd
			if (btn(â¬‡ï¸)) c.y+=c.spd
		else
			c.push_cursor_back_inside_radius(c.slct.x,c.slct.y)
		end
	else
		if (btn(â¬…ï¸)) c.x-=c.spd
		if (btn(âž¡ï¸)) c.x+=c.spd
		if (btn(â¬†ï¸)) c.y-=c.spd
		if (btn(â¬‡ï¸)) c.y+=c.spd
	end


	cursor_edge_detection()

	hover_handler()
	if (c.hvrd != nil) then 
		select_handler()
	end

	if (c.slct != nil) then
		dest_handler()
		if (c.dest != nil) then
			action_dispatcher()
		end
	end

	is_goal=b.is_goal(b)
	if (is_goal != nil) then
		if (is_goal == g.turn) then -- goal
			g.add_goal(g.turn)
		else -- own goal
			if (g.turn == 1) then
				g.add_goal(2)
			else 
				g.add_goal(1)
			end
		end
		b.update(g.pitch.xmid, g.pitch.ymid)
	end
end

function cursor_edge_detection()
	if (c.x >= g.pitch.xend-8) then
		c.x = g.pitch.xend-8
	end
	if (c.x <= g.pitch.xstart) then
		c.x = g.pitch.xstart
	end
	if (c.y >= g.pitch.yend-8) then
		c.y = g.pitch.yend-8
	end
	if (c.y <= g.pitch.ystart) then
		c.y = g.pitch.ystart
	end
end
	
--get hovered entity (player or ball)
function hover_handler()
	bhov=b.check_if_hovered(b)
	if (bhov) then
		c.hvrd=b
		return
	end
	for t in all(teams) do
		for p in all(t) do
			hov = p.check_if_hovered(p)
			if (hov) then
				-- only show hover effect if p is on same team or selection has been made
				-- opposition players can only be shown as 'hovered' if they are the destination
				if (p.t == g.turn or c.slct != nil) then
					p.hvrd=true
					c.hvrd = p
				end
			else
				p.hvrd=false
	 		end
	 	end
 	end
end

-- slct is the starting point of an action (i.e. the A of A-B)
-- determines if hovered entity can be selected
function select_handler()
	if (g.turn != c.hvrd.t or c.hvrd == b) then
	 return
	end

	if (btnp(âŽ)) then
		-- if hovering and no current selection
		if (c.hvrd != nil and c.slct == nil) then
			c.slct = c.hvrd
			sfx(1)
		end
	end
end

-- dest is the end point of an action (i.e. the B of A-B)
-- requires c.slct != nil to run
function dest_handler()
	action_type=""
	if (c.hvrd == b) then
		if (in_poss.t != c.slct.t) then
			c.dest=b
			action_type="steal"
	 	end
	elseif (c.hvrd == nil) then --if no player is hovered
		if (c.slct == in_poss) then
			c.dest = {x=c.x, y=c.y}
			action_type="kick"
		else
			c.dest = {x=c.x, y=c.y}
			action_type="run"
		end
	else
		if (c.slct.t == c.hvrd.t and c.slct != c.hvrd and c.slct == in_poss) then
			action_type = 'pass'
			c.dest = c.hvrd
		elseif (c.slct.t != c.hvrd.t) then
			action_type = 'tackle'
			c.dest = c.hvrd
		else
			action_type = ''
			spr(16,5,5)
		end
	end
end

--requires c.dest != nil to run
function action_dispatcher()
	if(btnp(âŽ)) then
		if (action_type == 'pass') then
			sfx(2)
			c.slct.make_pass(c.slct)
			c.dest.receive_pass(c.dest)
			b.update(c.dest.x+8,c.dest.y)
		elseif(action_type == 'kick') then
			sfx(3)
			b.update(c.x,c.y)
		elseif(action_type == 'run') then
			sfx(2)
			c.slct.update(c.slct,c.x,c.y)
		elseif(action_type == 'tackle') then
			c.slct.update(c.slct,c.dest.x,c.dest.y)
			c.dest.update(c.dest,c.dest.x-8,c.dest.y)
			if (c.dest == in_poss) then
				in_poss = c.slct
			end
		elseif(action_type == 'steal') then
			c.slct.update(c.slct,b.x,b.y)
			b.update(c.dest.x+8,c.dest.y)
			in_poss = c.slct
		else	
			--do nothing
		end
		end_of_turn_handler()
	end
end

function end_of_turn_handler()
	c.clear_all()
	g.end_turn()
end

-- duplication with player path?
function draw_ball_path()
		line(b.x+4,b.y+4,c.x+4,c.y+4,7)
		if(action_type != nil) then
			print(action_type,c.x,c.y-8,7)
		end
end

function draw_player_path()
		line(c.slct.x+4,c.slct.y+4,c.x+4,c.y+4,7)
		if(action_type != nil) then
			print(action_type,c.x,c.y-8,7)
		end
end


function _draw()
	cls()
	
	map(0,0)
	
	// players
	draw_all_players()
		
	// ball
	b.draw()
	
	if (c.slct != nil) then 
		if (c.slct == in_poss) then
			draw_ball_path()
		else
			draw_player_path()
		end
	end
		
	// cursor
	spr(17, c.x, c.y)
	
	info.draw_info_board()
	
	if (c.slct and c.dest) then
		info.log(c.slct.x .. " to " .. c.dest.x)
	end
	// crowd noise
	--sfx(2)
end
