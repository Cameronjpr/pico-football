function new_game()
	local g = {}
	
	g.turn = 1
	
	g.score = {}
	g.score[1] = 0
	g.score[2] = 0

	g.pitch={}

	

	g.add_goal = function(team)
		g.score[team] += 1
	end
	
	g.get_score = function()
		return g.score
	end
	
	g.end_turn = function()
		if g.turn == 1 then g.turn = 2
		else g.turn = 1
		end
	end
	
	g.get_turn = function()
		return g.turn
	end
		
	g.end_game = function(winner) 
		print("game over")
	end		
	
	g.draw_infobar = function()
		//print(tostr(g.score[1]) .. '-' .. tostr(g.score[2]),8,78,0)
	end
	
	sfx(0)
	return g
end
