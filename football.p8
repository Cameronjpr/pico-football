function new_player(t,n)
	local p = {}
	
	p.spr = t-1
	p.t = t
 p.n = n
 p.poss=false
 p.hvrd=false
 p.slct=false
 p.dest=false
 
	if n == 5 then
		p.spr=2
		p.x=teams[t]["gk_pos"]
		p.y=(g.pitch.height/2)+8+4
 else 
		p.x=teams[t]["pos"]
		p.y=((g.pitch.ystart - g.pitch.top_border) + (n*16) + 8)
	end

 p.draw = function(this)
  spr(this.spr, this.x, this.y)
 end
 
 p.update = function(this)
  // something
 end

 
 p.check_if_hovered = function(this)
 	x_hov=false
 	y_hov=false
 	
 	if (c.x+4 >= this.x and c.x+4 < this.x + 8) then
 		x_hov=true
 	end

 	if (c.y+4 >= this.y and c.y+4 < this.y + 8)	then
 		y_hov=true
 	end
 		
 	return x_hov and y_hov
 end			

 p.make_pass = function(this)
	this.poss=false
 end

 p.receive_pass = function(this)
	this.poss=true
 end
 	
 return p
end
