function _init()
	g = new_game()
	c = new_cursor()

	action_type=''
	
	teams = {}
	teams[1] = {}
	teams[2] = {}

	teams[1]["pos"] = g.pitch.xstart + g.pitch.side_border*2
	teams[2]["pos"] = g.pitch.xend - g.pitch.side_border*2-8
	teams[1]["gk_pos"] = g.pitch.side_border+4
	teams[2]["gk_pos"] = (g.pitch.width -  g.pitch.side_border)+4

	for x=1,5 do
		teams[1][x] = new_player(1,x);
		teams[2][x] = new_player(2,x);
	end
	
	b = new_ball()
	b.x = teams[1][2].x + 8
	b.y = teams[1][2].y
	teams[1][2].poss = true
end

function draw_all_players()
	for t in all(teams) do
  for p in all(t) do
			// hovered player
			if (p == c.hvrd) then
				spr(3,p.x,p.y)
			else
  		p.draw(p)
			end
			
			// selected player
			if (p == c.slct) then
				spr(4,p.x,p.y)
			// destination player 
		 	elseif (p == c.dest) then
				spr(5,p.x,p.y)
			end

			--possesion indicator
			if (p.poss) then
				spr(6,p.x,p.y-8)
			end
  end
 end
end
	
function _update()
	c.hvrd=nil
	
	if (btn(â¬…ï¸)) c.x-=1
	if (btn(âž¡ï¸)) c.x+=1
	if (btn(â¬†ï¸)) c.y-=1
	if (btn(â¬‡ï¸)) c.y+=1

	cursor_edge_detection()

	hover_handler()
	if (c.hvrd != nil) then 
		select_handler()
	end

	if (c.slct != nil) then
		dest_handler()
		if (c.dest != nil) then
			action_dispatcher()
		end
	end

	if (b.is_in_goal(b)) then
		g.add_goal(1)
		b.update(g.pitch.xmid, g.pitch.ymid)
	end
end

function cursor_edge_detection()
	if (c.x >= g.pitch.xend-8) then
		c.x = g.pitch.xend-8
	end
	if (c.x <= g.pitch.xstart) then
		c.x = g.pitch.xstart
	end
	if (c.y >= g.pitch.yend-8) then
		c.y = g.pitch.yend-8
	end
	if (c.y <= g.pitch.ystart) then
		c.y = g.pitch.ystart
	end
end
	
--get hovered entity (player)
function hover_handler()
	for t in all(teams) do
		for p in all(t) do
			hov = p.check_if_hovered(p)
			if (hov) then
				p.hvrd=true
				c.hvrd = p
			else
				p.hvrd=false
	 		end
	 	end
 	end
end

-- slct is the starting point of an action (i.e. the A of A-B)
function select_handler()
	if (g.turn != c.hvrd.t) then
	 return
	end

	if (btnp(âŽ)) then
		if (c.hvrd != nil and c.slct == nil) then
			c.hvrd.slct=true
			c.slct = c.hvrd
			sfx(1)
		elseif (c.hvrd != nil and c.slct != nil and c.dest == nil) then
			c.dest = c.hvrd
			c.hvrd.dest=true
		else
			spr(16,5,5)
		end
	elseif(btnp(ðŸ…¾ï¸)) then
		c.clear_all()
	end
end

-- dest is the end point of an action (i.e. the B of A-B)
-- requires c.slct != nil to run
function dest_handler()
	action_type=""
	if (c.hvrd == nil) then --if no player is hovered
		if (c.slct.poss) then
			c.dest = {x=c.x, y=c.y}
			action_type="kick"
		else
			c.dest = {x=c.x, y=c.y}
			action_type="run"
		end
	else
		if (c.slct.t == c.hvrd.t and c.slct != c.hvrd and c.slct.poss) then
			action_type = 'pass'
			c.dest = c.hvrd
			c.hvrd.dest=true
		elseif (c.slct.t != c.hvrd.t) then
			action_type = 'tackle'
		else
			action_type = ''
			spr(16,5,5)
		end
	end
end

--requires c.dest != nil to run
function action_dispatcher()
	if(btnp(âŽ)) then
		if (action_type == 'pass') then
			c.slct.make_pass(c.slct)
			c.dest.receive_pass(c.dest)
			sfx(2)
			b.update(c.dest.x+8,c.dest.y)
		elseif(action_type == 'kick') then
			sfx(3)
			b.update(c.x,c.y)
		end
		end_of_turn_handler()
	end
end

function end_of_turn_handler()
	c.clear_all()
	g.end_turn()
end

function draw_ball_path()
		line(b.x+4,b.y+4,c.x+4,c.y+4)
		if(action_type != nil) then
			print(action_type,c.x,c.y-8)
		end
end

function draw_player_path()
		line(c.slct.x+4,c.slct.y+4,c.x+4,c.y+4)
		if(action_type != nil) then
			print(action_type,c.x,c.y-8)
		end
end


function _draw()
	cls()
	
	map(0,0)
	
	// players
	draw_all_players()
		
	// ball
	b.draw()
	
	// ball_path
	if (c.slct != nil) then 
		if (c.slct.poss) then
			draw_ball_path()
		else
			draw_player_path()
		end
	end
		
	// cursor
	spr(17, c.x, c.y)
	
	g.draw_infobar()
	
	// crowd noise
	--sfx(2)
end
