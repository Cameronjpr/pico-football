function _init()
	g = new_game()
	p_width = 128;
	p_height = 64;
	
	c = new_cursor()
	
	teams = {}
	teams[1] = {}
	teams[2] = {}

	teams[1]["pos"] = p_width*0.25 //32 
	teams[2]["pos"] = p_width*0.75 //96
	teams[1]["gk_pos"] = 5
	teams[2]["gk_pos"] = p_width - 5 - 8

	for x=1,5 do
		teams[1][x] = new_player(1,x);
		teams[2][x] = new_player(2,x);
	end
	
	b = new_ball()
	b.x = teams[1][2].x + 8
	b.y = teams[1][2].y
	teams[1][2].poss = true
end

function draw_all_players()
	for t in all(teams) do
  for p in all(t) do
			// hovered player
			if (p == c.hvrd) then
				spr(3,p.x,p.y)
			else
  		p.draw(p)
			end
			
			// selected player
			if (p == c.slct) then
				spr(4,p.x,p.y)
			// destination player 
		 elseif (p == c.dest) then
				spr(5,p.x,p.y)
			end
  end
 end
end
	
function _update()
	c.hvrd=nil
	
	if (btn(⬅️)) c.x-=1
	if (btn(➡️)) c.x+=1
	if (btn(⬆️)) c.y-=1
	if (btn(⬇️)) c.y+=1

	hover_handler()
	select_handler()
	if (c.slct != nil) then
		dest_handler()
	end
end

function hover_handler()
	for t in all(teams) do
		for p in all(t) do
			hov = p.check_if_hovered(p)
			if (hov) then
	 		p.hvrd=true
	 		c.hvrd = p
	 	else
	 		p.hvrd=false
	 	end
	 end
 end
end

function select_handler()
	if (btnp(❎)) then
		if (c.hvrd != nil and c.slct == nil) then
			c.hvrd.slct=true
			c.slct = c.hvrd
			sfx(1)
		elseif (c.hvrd != nil and c.slct != nil and c.dest == nil) then
			c.dest = c.hvrd
			c.hvrd.dest=true
		else
			spr(16,5,5)
		end
	end
end

function dest_handler()
	if (c.hvrd == nil) then
		c.dest = nil
	elseif (c.hvrd != nil and c.slct != nil) then
			c.dest = c.hvrd
			c.hvrd.dest=true
			g.end_turn()
		else
			spr(16,5,5)
		end
end

function draw_ball_path()
		line(c.slct.x+4,c.slct.y+4,c.x+4,c.y+4)
end
		

function _draw()
	cls()
	
	map(0,0)
	
	// players
	draw_all_players()
		
	// ball
	b.draw()
	
	// ball_path
	if (c.slct != nil) then 
		draw_ball_path()
	end
		
	// cursor
	spr(17, c.x, c.y)
	
	if (c.hvrd == nil) then
		spr(16,74,64)
	else
		spr(16,64,64)
	end	
	
	g.draw_infobar()
	
	// crowd noise
	// sfx(2)
end
