function new_ball()
	local b={}
	
	b.diam=8
	b.x=64
	b.y=32
	b.hov=false

	b.update = function(x,y)
		b.x=x
		b.y=y
	end
	
	b.draw=function()
		if (bhov) then
			spr(19,b.x,b.y)
		else
			spr(16,b.x,b.y)
		end
	end
	
	b.is_in_goal=function(this)
		is_goal=false

		if (b.x <= g.goalpost_left.line) then
			if (b.y < g.goalpost_left.bottom_post and b.y > g.goalpost_left.top_post) then
				is_goal=true
			end
		end

		return is_goal
	end

	b.check_if_hovered = function(this)
		x_hov=false
 		y_hov=false
 	
		if (c.x+4 >= this.x and c.x+4 < this.x + 8) then
			x_hov=true
		end

		if (c.y+4 >= this.y and c.y+4 < this.y + 8)	then
			y_hov=true
		end
			
		b.hov=(x_hov and y_hov)	
		return x_hov and y_hov	
	end
	
	return b
end
