function new_ball()
	local b={}
	
	b.diam=8
	b.x=64
	b.y=32
	
	b.update = function(x,y)
		b.x=x
		b.y=y
	end
	
	b.draw=function()
		spr(16,b.x,b.y)
	end
	
	b.is_in_goal=function()
		//
	end
	
	return b
end
