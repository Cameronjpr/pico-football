function new_cursor()
	local c={}
	
	c.x=48
	c.y=64
	
	// hovered
	c.hvrd = {}
	
	// selected
	c.slct = nil
	
	// destination
	c.dest= nil
	
	c.set_hvrd = function(t,n)
		c.hvrd = teams[t][n]
	end
	
	c.set_slct = function(t,n)
		c.slct = teams[t][n]
	end
	
	c.set_dest = function(x,y)
		// todo - coords or n? 
	end
	
	return c
end

